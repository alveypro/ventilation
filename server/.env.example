# https://your-openclaw-domain/v1/chat/completions
OPENCLAW_CHAT_URL=
OPENCLAW_API_KEY=
OPENCLAW_AUTH_HEADER=Authorization
OPENCLAW_AUTH_SCHEME=Bearer
# openai | raw
OPENCLAW_REQUEST_MODE=openai
OPENCLAW_MODEL=qwen-plus
OPENCLAW_TIMEOUT_MS=45000
OPENCLAW_RETRIES=1
